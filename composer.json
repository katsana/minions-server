{
    "name": "katsana/minions-server",
    "description": "Minions Component Server using ReactPHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@katsana.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Minions\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minions\\Server\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/console": "^6.0 || ^7.0 || ^8.0",
        "katsana/minions": "^1.6 || ^2.1",
        "laravie/stream": "^1.3",
        "league/climate": "^3.5",
        "react/event-loop": "^1.1",
        "react/http": "^0.8.4",
        "react/promise": "^2.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^4.5 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.4 || ^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "laravel": {
            "providers": [
                "Minions\\Server\\MinionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
